[Argument]

[rule]

[rewrite]
(?i)^https:\/\/([0-9a-z]+\.)?kelee\.one response-header-replace Content-Type text/plain;charset=utf-8
(?i)^https:\/\/([0-9a-z]+\.)?kelee\.one header-replace User-Agent script-hub/1.0.0

[script]

[mitm]
hostname= rule.kelee.one,kelee.one